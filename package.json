  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "client:dev": "vite --config vite.client.config.ts --port 5173 --host",
    "tunnel": "cloudflared tunnel --config cloudflared/config.yml run",
    "build": "vite build && cross-env NODE_ENV=production esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --external:vite",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "admin:dev": "cd admin-panel && npm run dev",
    "admin:build": "cd admin-panel && npm run build",
    "admin:start": "cd admin-panel && npm run start"
  },
